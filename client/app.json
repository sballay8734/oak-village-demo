{
  "expo": {
    "name": "oak-village-demo",
    "slug": "oak-village-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "userInterfaceStyle": "light"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", [
        "expo-font", {"fonts": ["./assets/fonts/Poppins-Bold.ttf", "./assets/fonts/Poppins-Medium.ttf", "./assets/fonts/RobotoMono-VariableFont_wght.ttf", "./assets/fonts/Mooli-Regular.ttf"]}
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
